{
  "headers": [
    {
      "source": "/public/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "immutable, max-age=604800"
        }
      ]
    },
    {
      "source": "/(.*)\\.(svg|jpg|png|gif|webp|ico|woff2|woff|ttf|eot|webmanifest)",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, max-age=604800, immutable"
        },
        {
          "key": "cache-control",
          "value": "public, max-age=604800, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.(css|js)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, stale-while-revalidate=604800"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "x-content-type-options",
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1;mode=block"
        }
      ]
    }
  ]
}